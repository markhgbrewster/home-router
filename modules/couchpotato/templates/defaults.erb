CP_USER=couchpotato        #$RUN_AS, username to run sickbeard under, the default is sickbeard
CP_HOME=<%= @couchpotato_root %>        #$APP_PATH, the location of SickBeard.py, the default is /opt/sickbeard
CP_DATA=/etc/couchpotato         #$DATA_DIR, the location of sickbeard.db, cache, logs, the default is /opt/sickbeard
## CP_PIDFILE=      #$PID_FILE, the location of sickbeard.pid, the default is /var/run/sickbeard/sickbeard.pid
## PYTHON_BIN=      #$DAEMON, the location of the python binary, the default is /usr/bin/python
## SSD_OPTS=        #$EXTRA_SSD_OPTS, extra start-stop-daemon option like " --group=users"
