SB_USER=sickbeard        #$RUN_AS, username to run sickbeard under, the default is sickbeard
SB_HOME=<%= @sickbeard_root %>        #$APP_PATH, the location of SickBeard.py, the default is /opt/sickbeard
SB_DATA=/etc/sickbeard         #$DATA_DIR, the location of sickbeard.db, cache, logs, the default is /opt/sickbeard
## SB_PIDFILE=      #$PID_FILE, the location of sickbeard.pid, the default is /var/run/sickbeard/sickbeard.pid
## PYTHON_BIN=      #$DAEMON, the location of the python binary, the default is /usr/bin/python
SB_OPTS=--port=9092         #$EXTRA_DAEMON_OPTS, extra cli option for sickbeard, i.e. " --config=/home/sickbeard/config.ini"
## SSD_OPTS=        #$EXTRA_SSD_OPTS, extra start-stop-daemon option like " --group=users"
